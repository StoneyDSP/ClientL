{
    "version": 5,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 24,
        "patch": 2
    },
    "configurePresets": [
        {
            "name": "LinuxVendorMap",
            "vendor": {
                "microsoft.com/VisualStudioSettings/CMake/1.0": {
                    "hostOS": "Linux",
                    "enableMicrosoftCodeAnalysis": true,
                    "enableClangTidyCodeAnalysis": false,
                    "cacheRoot": "${sourceDir}/bin/${presetName}",
                    "intelliSenseOptions": {
                        "useCompilerDefaults": true
                    }
                },
                "microsoft.com/VisualStudioRemoteSettings/CMake/1.0": {
                    "sourceDir": "$env{HOME}/.vs/${sourceDirName}",
                    "copyBuildOutput": false,
                    "copySources": true,
                    "forceWSL1Toolset": false,
                    "rsyncCommandArgs": [
                        "-t",
                        "--delete",
                        "--delete-excluded"
                    ],
                    "copyOptimizations": {
                        "useOptimizations": "RsyncAndSftp",
                        "maxSmallChange": 10,
                        "rsyncSingleDirectoryCommandArgs": [
                            "-t",
                            "-d"
                        ]
                    },
                    "copyExcludeDirectoriesList": [],
                    "copyAdditionalIncludeDirectoriesList": [],
                    "copySourcesOptions": {
                        "method": "rsync",
                        "concurrentCopies": 5,
                        "outputVerbosity": "Diagnostic",
                        "exclusionList": [
                            ".vs",
                            ".git",
                            "out",
                            "build",
                            "installed",
                            "node_modules"
                        ]
                    }
                }
            }
        }
    ],
    "vendor": {
        "microsoft.com/VisualStudioSettings/CMake/1.0": {
            "hostOS": "Linux",
            "enableMicrosoftCodeAnalysis": true,
            "enableClangTidyCodeAnalysis": false,
            "cacheRoot": "${sourceDir}/bin/${presetName}",
            "intelliSenseOptions": {
                "useCompilerDefaults": true
            }
        },
        "microsoft.com/VisualStudioRemoteSettings/CMake/1.0": {
            "sourceDir": "$env{HOME}/.vs/${sourceDirName}",
            "copyBuildOutput": false,
            "copySources": true,
            "forceWSL1Toolset": false,
            "rsyncCommandArgs": [
                "-t",
                "--delete",
                "--delete-excluded"
            ],
            "copyOptimizations": {
                "useOptimizations": "RsyncAndSftp",
                "maxSmallChange": 10,
                "rsyncSingleDirectoryCommandArgs": [
                    "-t",
                    "-d"
                ]
            },
            "copyExcludeDirectoriesList": [],
            "copyAdditionalIncludeDirectoriesList": [],
            "copySourcesOptions": {
                "method": "rsync",
                "concurrentCopies": 5,
                "outputVerbosity": "Diagnostic",
                "exclusionList": [
                    ".vs",
                    ".git",
                    "out",
                    "build",
                    "installed",
                    "node_modules"
                ]
            }
        }
    }
}
